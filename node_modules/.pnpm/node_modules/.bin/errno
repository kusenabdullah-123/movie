#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/errno@0.1.7/node_modules/errno/cli.js/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/errno@0.1.7/node_modules/errno/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/errno@0.1.7/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules:/home/kusen/nodejs/webpack/movie/node_modules:/home/kusen/nodejs/webpack/node_modules:/home/kusen/nodejs/node_modules:/home/kusen/node_modules:/home/node_modules:/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/errno/cli.js/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/errno/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../errno/cli.js" "$@"
else 
  exec node  "$basedir/../errno/cli.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/bin/acorn/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/bin/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/acorn@6.4.1/node_modules/acorn/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/acorn@6.4.1/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules:/home/kusen/nodejs/webpack/movie/node_modules:/home/kusen/nodejs/webpack/node_modules:/home/kusen/nodejs/node_modules:/home/kusen/node_modules:/home/node_modules:/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/acorn/bin/acorn/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/acorn/bin/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/acorn/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi

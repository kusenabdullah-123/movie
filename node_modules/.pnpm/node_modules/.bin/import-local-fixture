#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/cli.js/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/fixtures/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/import-local@2.0.0/node_modules/import-local/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/import-local@2.0.0/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules:/home/kusen/nodejs/webpack/movie/node_modules:/home/kusen/nodejs/webpack/node_modules:/home/kusen/nodejs/node_modules:/home/kusen/node_modules:/home/node_modules:/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/import-local/fixtures/cli.js/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/import-local/fixtures/node_modules:/home/kusen/nodejs/webpack/movie/node_modules/.pnpm/node_modules/import-local/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else 
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
